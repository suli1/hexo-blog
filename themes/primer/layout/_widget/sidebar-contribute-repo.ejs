<% if (theme.profile.github && theme.github && theme.github.contribute_repos) { %>
    <div class="boxed-group flush" role="navigation">
        <h3>Repositories contribute to</h3>
        <ul class="boxed-group-inner mini-repo-list">
            <% for (var i in theme.github.contribute_repos) { %>
                <li class="public source ">
                    <a href="https://github.com/<%= theme.github.contribute_repos[i] %>" target="_blank" title="<%= theme.github.contribute_repos[i] %>" class="mini-repo-list-item css-truncate">
                        <span class="repo-icon octicon octicon-repo"></span>
                        <span class="repo-and-owner css-truncate-target">
                           <%= theme.github.contribute_repos[i] %>
                        </span>
                    </a>
                </li>
            <% } %>
        </ul>
    </div>
<% } %>

